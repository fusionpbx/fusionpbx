#!/bin/sh
set -e

case "$1" in
  remove|upgrade|deconfigure)
    if [ -d /var/www/fusionpbx/themes/default ]; then
      echo "We're about to remove fusionpnx minimal theme..." >&2
    fi
    ;;
  failed-upgrade)
    ;;
  *)
    echo "prerm called with unknown argument \`$1'" >&2
    exit 1
    ;;
esac

#DEBHELPER#
exit 0
