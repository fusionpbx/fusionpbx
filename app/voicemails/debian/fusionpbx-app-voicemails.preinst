#!/bin/sh
set -e

case "$1" in
  install|upgrade)
    if [ -d /var/www/fusionpbx/app/voicemail ]; then
      echo "It looks like you have FusionPBX voicemail app installed !." >&2
    fi
    ;;
  abort-upgrade)
    ;;
  *)
    echo "preinst called with unknown argument \`$1'" >&2
    exit 1
    ;;
esac

#DEBHELPER#
exit 0
