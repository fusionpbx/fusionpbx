<extension name="is_whitelisted" number="" context="global" continue="true" app_uuid="59a84f1f-6f7d-4a88-8e06-75427e3e6209" enabled="false" order="41">
	<condition field="${is_whitelisted_checked}" expression="^true$" break="on-true">
		<action application="log" data="Skipping whitelist validation since it's already checked."/>
	</condition>
	<condition field="destination_number" expression="^\+?1?(\d{10})$" break="never">
		<action application="set" data="is_whitelisted_checked=true"/>
		<action application="lua" data="is_whitelisted.lua $1"/>
	</condition>
	<condition field="destination_number" expression="^(\d{1,6})$" break="never">
		<action application="set" data="is_whitelisted_checked=true"/>
		<action application="lua" data="is_whitelisted.lua $1"/>
	</condition>
</extension>
